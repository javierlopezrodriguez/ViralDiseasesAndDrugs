###  https://w3id.org/def/ViralDiseasesAndDrugs#affects
:affects rdf:type owl:ObjectProperty ;
         owl:inverseOf :isAffectedBy ;
         rdfs:comment "A relation between two entities in which one has an influence on the other" ;
         rdfs:label "affects" .


###  https://w3id.org/def/ViralDiseasesAndDrugs#causes
:causes rdf:type owl:ObjectProperty ;
        owl:inverseOf :isCausedBy ;
        rdfs:comment "A relation between two entities in which one originates the other" ;
        rdfs:label "causes" .


###  https://w3id.org/def/ViralDiseasesAndDrugs#commercializesDrug
:commercializesDrug rdf:type owl:ObjectProperty ;
                    rdfs:comment "A relation between the class that represents the n-ary relationship of commercializing a drug with a name in a country; and the commercialized drug." ;
                    rdfs:label "commercializes drug" .


###  https://w3id.org/def/ViralDiseasesAndDrugs#commercializesDrugWithNameInCountry
:commercializesDrugWithNameInCountry rdf:type owl:ObjectProperty ;
                                     rdfs:comment "A relation between a pharmaceutical company and the class that represents the n-ary relationship of commercializing a drug with a name in a country." ;
                                     rdfs:label "commercializes drug with name in country" .


###  https://w3id.org/def/ViralDiseasesAndDrugs#commercializesInCountry
:commercializesInCountry rdf:type owl:ObjectProperty ;
                         rdfs:comment "A relation between the class that represents the n-ary relationship of commercializing a drug with a name in a country; and the country where the drug is commercialized." ;
                         rdfs:label "commercializes in country" .


###  https://w3id.org/def/ViralDiseasesAndDrugs#containsParagraph
:containsParagraph rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf <https://w3id.org/def/DRUGS4COVID19#contains> ;
                   rdfs:range <https://w3id.org/def/DRUGS4COVID19#Paragraph> ;
                   rdfs:comment "A relation between a text and one of its paragraphs" ;
                   rdfs:label "contains paragraph" .


###  https://w3id.org/def/ViralDiseasesAndDrugs#containsSentence
:containsSentence rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf <https://w3id.org/def/DRUGS4COVID19#contains> ;
                  rdfs:range <https://w3id.org/def/DRUGS4COVID19#Sentence> ;
                  rdfs:comment "A relation between a text and one of its sentences" ;
                  rdfs:label "contains sentence" .


###  https://w3id.org/def/ViralDiseasesAndDrugs#destroys
:destroys rdf:type owl:ObjectProperty ;
          owl:inverseOf :isDestroyedBy ;
          rdfs:comment "A relation between two entities in which one destroys or inhibits the other" ;
          rdfs:label "destroys" .


###  https://w3id.org/def/ViralDiseasesAndDrugs#drugGivenByDoctor
:drugGivenByDoctor rdf:type owl:ObjectProperty ;
                   owl:inverseOf :givesDrug ;
                   rdfs:comment "A relation between a drug and the doctor who gives it as treatment" ;
                   rdfs:label "drug given by doctor" .


###  https://w3id.org/def/ViralDiseasesAndDrugs#drugMarketedByCompany
:drugMarketedByCompany rdf:type owl:ObjectProperty ;
                       owl:inverseOf :marketsDrug ;
                       rdfs:comment "A relation between a drug and the pharmaceutical company that commercializes it" ;
                       rdfs:label "drug marketed by company" .


###  https://w3id.org/def/ViralDiseasesAndDrugs#givesDrug
:givesDrug rdf:type owl:ObjectProperty ;
           rdfs:comment "A relation between a doctor and a drug they give as treatment" ;
           rdfs:label "gives drug" .


###  https://w3id.org/def/ViralDiseasesAndDrugs#hasDirector
:hasDirector rdf:type owl:ObjectProperty ;
             owl:inverseOf :isDirectorOfCompany ;
             rdfs:range :Director ;
             rdfs:comment "A relation in which an organization has a director" ;
             rdfs:label "has director" .


###  https://w3id.org/def/ViralDiseasesAndDrugs#hasHeadquarters
